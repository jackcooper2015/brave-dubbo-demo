<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context" xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

     <context:property-placeholder location="classpath:dubbo.properties" ignore-unresolvable="true"/>
<!--     公共信息，也可以用dubbo.properties配置 -->
     <dubbo:application name="brave-dubbo-consumer"/>
     <dubbo:reference id="iGwOrdersService" interface="com.reapal.gworder.service.IGwOrdersService" check="false" timeout="60000" retries="0"  version="${dubbo.version}" />


</beans>
